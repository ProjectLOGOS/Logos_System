# LOGOS LLM Configuration
# Copy this file to .env and add your actual API keys

# LLM Provider: openai, anthropic, groq, or mock
LOGOS_LLM_PROVIDER=mock

# Groq API Key (free tier - get from https://console.groq.com)
# GROQ_API_KEY=gsk_your_key_here

# Debug mode (set to true for verbose output)
DEBUG=false

# OpenAI API Key (if using OpenAI)
# OPENAI_API_KEY=sk-your_key_here

# Anthropic API Key (if using Anthropic)
# ANTHROPIC_API_KEY=sk-ant-your_key_here

# Optional: Override default model
# LOGOS_LLM_MODEL=llama-3.3-70b-versatile
