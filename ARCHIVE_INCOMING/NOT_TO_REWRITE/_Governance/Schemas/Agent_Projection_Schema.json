{
  "schema_version": "1.0",
  "description": "Unified governance schema for agent ontology/epistemology projections in Phase-F.",
  "required_fields": [
    "projection_id",
    "agent_identity",
    "bound_protocol",
    "semantic_principle",
    "mvs_axis",
    "governance",
    "canonical_sources",
    "projection_rules",
    "read_constraints",
    "shared_protocols_bound",
    "explicit_prohibitions",
    "audit"
  ],
  "constraints": {
    "governance.read_only": true,
    "governance.executable": false,
    "audit.hash_verified": true,
    "audit.review_required_for_change": true
  },
  "enforcement": "fail_closed"
}
