{
  "contract": "Phase-I Execution Contract",
  "status": "AUTHORITATIVE \u00b7 NON-BYPASSABLE",
  "phase": "Phase-I",
  "scope": "Governed Routing Surfaces (Enrichment Only)",
  "ordering_guarantee": [
    "LOGOS_AGENT_READY",
    "PHASE_H_COMPLETE",
    "ROUTING_SURFACES_AVAILABLE"
  ],
  "identity_invariants": {
    "identity_state": "FROZEN",
    "permitted_hashes": [
      "universal_session_id",
      "logos_agent_id",
      "I1_agent_id",
      "I2_agent_id",
      "I3_agent_id",
      "artifact_hashes"
    ],
    "explicit_prohibitions": [
      "identity_issuance",
      "identity_derivation",
      "identity_rebinding",
      "hash_rotation",
      "authority_escalation"
    ]
  },
  "routing_surfaces": {
    "route_for_enrichment": {
      "owner": "LOGOS",
      "purpose": "Fan-out governed artifacts to enrichment protocols",
      "inputs": [
        "Governed_Artifact",
        "session_id",
        "logos_id"
      ],
      "outputs": [
        "Enrichment_Request_Set"
      ],
      "constraints": [
        "no_state_mutation",
        "no_protocol_execution",
        "no_branching_on_content",
        "read_only"
      ]
    },
    "receive_enrichment_delta": {
      "owner": "LOGOS",
      "purpose": "Receive advisory enrichment output",
      "inputs": [
        "artifact_id",
        "protocol_id",
        "Enrichment_Delta",
        "session_id",
        "logos_id"
      ],
      "outputs": [
        "Acknowledgement"
      ],
      "constraints": [
        "delta_is_non_authoritative",
        "no_commands",
        "no_state_mutation",
        "read_only"
      ]
    },
    "collect_enrichment_deltas": {
      "owner": "LOGOS",
      "purpose": "Aggregate enrichment deltas",
      "inputs": [
        "artifact_id",
        "List[Enrichment_Delta]"
      ],
      "outputs": [
        "Enriched_Artifact"
      ],
      "constraints": [
        "no_interpretation",
        "no_weighting",
        "no_prioritization",
        "pure_aggregation"
      ]
    },
    "finalize_routing_cycle": {
      "owner": "LOGOS",
      "purpose": "Close routing cycle and return artifact for SMP synthesis",
      "inputs": [
        "Enriched_Artifact",
        "session_id",
        "logos_id"
      ],
      "outputs": [
        "Routed_Artifact"
      ],
      "constraints": [
        "no_decision_making",
        "no_persistence",
        "no_state_mutation"
      ]
    }
  },
  "smp_alignment": {
    "governing_schema": "SMP_Pipeline_Governance_Addendum.schema.json",
    "governing_policy": "SMP_Pipeline_Governance_Addendum.policy.json",
    "rule": "routing_must_not_bypass_smp"
  },
  "filesystem_constraints": {
    "write_scope": "Logos_System_Rebuild",
    "violation": "immediate_halt"
  },
  "failure_semantics": {
    "on_violation": [
      "halt_execution",
      "discard_partial_routing_state",
      "emit_audit_event"
    ],
    "mode": "FAIL_CLOSED"
  },
  "explicit_non_goals": [
    "protocol_activation",
    "protocol_execution",
    "learning",
    "memory_write",
    "agent_to_agent_communication",
    "external_io"
  ],
  "explicit_prohibitions": [
    "protocol_activation"
  ]
}