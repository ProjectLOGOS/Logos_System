{
  "contract": "Phase-H Execution Contract",
  "status": "AUTHORITATIVE \u00b7 NON-BYPASSABLE",
  "phase": "Phase-H",
  "scope": "Controlled Protocol Activation (Identity-Frozen)",
  "ordering_guarantee": [
    "LOGOS_AGENT_READY",
    "AGENT_ORCHESTRATION_AND_PROTOCOL_BINDING",
    "CONTROLLED_PROTOCOL_ACTIVATION"
  ],
  "identity_invariants": {
    "identity_state": "FROZEN",
    "permitted_identities": [
      "universal_session_id",
      "logos_agent_id",
      "I1_agent_id",
      "I2_agent_id",
      "I3_agent_id",
      "protocol_bound_hashes"
    ],
    "explicit_prohibitions": [
      "identity_issuance",
      "identity_derivation",
      "identity_rebinding",
      "hash_rotation",
      "authority_escalation"
    ]
  },
  "activation_targets": {
    "SCP": {
      "bound_agent": "I1",
      "activation_mode": "constrained",
      "permissions": [
        "internal_execution"
      ],
      "prohibitions": [
        "external_io",
        "cross_protocol_access"
      ]
    },
    "MTP": {
      "bound_agent": "I2",
      "activation_mode": "constrained",
      "permissions": [
        "internal_execution"
      ],
      "prohibitions": [
        "external_io",
        "cross_protocol_access"
      ]
    },
    "ARP": {
      "bound_agent": "I3",
      "activation_mode": "constrained",
      "permissions": [
        "internal_execution"
      ],
      "prohibitions": [
        "external_io",
        "cross_protocol_access"
      ]
    }
  },
  "shared_substrate_constraints": {
    "Cognitive_State_Protocol": {
      "access_control": {
        "LOGOS_AGENT": "read_write",
        "I1": "read_only",
        "I2": "read_only",
        "I3": "read_only",
        "SOP": "forbidden",
        "external": "forbidden"
      },
      "identity_requirement": "pre-existing_hashes_only"
    }
  },
  "safety_and_rollback": {
    "pre_activation_checks": [
      "identity_frozen",
      "ordering_satisfied",
      "filesystem_preflight_passed"
    ],
    "on_failure": [
      "deactivate_all_protocols",
      "preserve_identity_state",
      "emit_diagnostic_event",
      "halt_fail_closed"
    ]
  },
  "filesystem_constraints": {
    "write_scope": "Logos_System_Rebuild",
    "violation": "immediate_halt"
  },
  "failure_semantics": "FAIL_CLOSED",
  "explicit_prohibitions": [
    "protocol_activation"
  ]
}