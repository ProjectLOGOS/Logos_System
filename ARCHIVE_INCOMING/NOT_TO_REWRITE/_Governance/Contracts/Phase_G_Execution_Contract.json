{
  "contract": "Phase-G Execution Contract",
  "status": "AUTHORITATIVE - NON-BYPASSABLE",
  "phase": "Phase-G",
  "scope": "Agent Initialization, Protocol Binding, and Shared Substrate Access",
  "ordering_guarantee": [
    "LOGOS_AGENT_READY",
    "AGENT_ORCHESTRATION_AND_PROTOCOL_BINDING"
  ],
  "identity_lattice": {
    "universal_session_id": {
      "issued_by": "Lock-and-Key (dual compile)",
      "required_for": [
        "LOGOS Agent",
        "I1",
        "I2",
        "I3",
        "SOP",
        "Cognitive_State_Protocol"
      ]
    },
    "logos_agent_id": {
      "issued_by": "Constructive LEM discharge",
      "required_for": [
        "I1",
        "I2",
        "I3",
        "Logos_Protocol",
        "SOP",
        "Cognitive_State_Protocol"
      ],
      "prohibitions": [
        "protocol_execution",
        "agent_impersonation",
        "authority_escalation"
      ]
    },
    "agent_ids": {
      "I1_agent_id": {
        "bound_protocol": "SCP",
        "requires": "logos_agent_id"
      },
      "I2_agent_id": {
        "bound_protocol": "MTP",
        "requires": "logos_agent_id"
      },
      "I3_agent_id": {
        "bound_protocol": "ARP",
        "requires": "logos_agent_id"
      }
    }
  },
  "agents": {
    "I1": {
      "protocol": "SCP",
      "permitted_actions_at_init": [
        "binding_only"
      ],
      "execution": "forbidden",
      "memory": "forbidden",
      "planning": "forbidden",
      "external_io": "forbidden",
      "governance": [
        "exclusive_protocol_binding",
        "fail_closed",
        "reordering_forbidden"
      ]
    },
    "I2": {
      "protocol": "MTP",
      "permitted_actions_at_init": [
        "binding_only"
      ],
      "execution": "forbidden",
      "memory": "forbidden",
      "planning": "forbidden",
      "external_io": "forbidden",
      "governance": [
        "exclusive_protocol_binding",
        "fail_closed",
        "reordering_forbidden"
      ]
    },
    "I3": {
      "protocol": "ARP",
      "permitted_actions_at_init": [
        "binding_only"
      ],
      "execution": "forbidden",
      "memory": "forbidden",
      "planning": "forbidden",
      "external_io": "forbidden",
      "governance": [
        "exclusive_protocol_binding",
        "fail_closed",
        "reordering_forbidden"
      ]
    }
  },
  "logos_agent": {
    "protocol": "Logos orchestration channel only",
    "permitted_actions": [
      "route_messages",
      "manage_lifecycle",
      "query_health"
    ],
    "explicit_permissions": {
      "SOP": "read_write",
      "Cognitive_State_Protocol": "read_write"
    },
    "prohibitions": [
      "protocol_internal_execution",
      "acting_as_I1_I2_I3",
      "authority_delegation"
    ],
    "failure_semantics": "FAIL_CLOSED"
  },
  "sop": {
    "role": "diagnostics_and_meta_governance_only",
    "identity_inputs": [
      "universal_session_id",
      "logos_agent_id"
    ],
    "permissions": [
      "diagnostics",
      "telemetry"
    ],
    "explicit_prohibitions": [
      "agent_interaction",
      "protocol_execution",
      "memory_access",
      "external_io"
    ],
    "isolation": "airlocked",
    "failure_semantics": "FAIL_CLOSED"
  },
  "shared_substrate_protocols": {
    "Cognitive_State_Protocol": {
      "classification": "shared_substrate - non-agent",
      "identity_inputs": [
        "universal_session_id",
        "logos_agent_id",
        "I1_agent_id",
        "I2_agent_id",
        "I3_agent_id"
      ],
      "access_control": {
        "LOGOS_AGENT": "read_write",
        "I1": "read_only",
        "I2": "read_only",
        "I3": "read_only",
        "SOP": "forbidden",
        "external": "forbidden"
      },
      "capabilities": [
        "state_read",
        "state_rehydration"
      ],
      "explicit_prohibitions": [
        "identity_issuance",
        "reasoning_execution",
        "planning_execution",
        "cross_agent_mutation",
        "authority_escalation"
      ],
      "failure_semantics": "FAIL_CLOSED"
    }
  },
  "global_prohibitions": [
    "protocol_activation",
    "agent_execution",
    "memory_mutation_by_non_logos",
    "cross_protocol_access",
    "external_io"
  ],
  "filesystem_constraints": {
    "write_scope": "Logos_System_Rebuild",
    "legacy_access": "read_only_or_forbidden",
    "violation": "immediate_halt"
  },
  "failure_semantics": "FAIL_CLOSED",
  "explicit_prohibitions": [
    "protocol_activation"
  ]
}